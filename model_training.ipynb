    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# WCSS = Within-Cluster Sum of Squares\\n\",\n",
    "    \"wcss = []\\n\",\n",
    "    \"k_range = range(1, 11) # testing between 1 and 10 clusters\\n\",\n",
    "    \"for k in k_range:\\n\",\n",
    "    \"    kmeans = KMeans(n_clusters=k, init='k-means++', n_init=10, random_state=0)\\n\",\n",
    "    \"    kmeans.fit(rfm_scaled)\\n\",\n",
    "    \"    wcss.append(kmeans.inertia_)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# plot elbow graph\\n\",\n",
    "    \"plt.figure(figsize=(10, 6))\\n\",\n",
    "    \"plt.plot(k_range, wcss, marker='o', linestyle='--')\\n\",\n",
    "    \"plt.title('Método do Cotovelo (Elbow Method)')\\n\",\n",
    "    \"plt.xlabel('Número de Clusters (K)')\\n\",\n",
    "    \"plt.ylabel('WCSS (Inércia)')\\n\",\n",
    "    \"plt.xticks(k_range)\\n\",\n",
    "    \"plt.grid(True)\\n\",\n",
    "    \"plt.show()\\n\"\n",
    "   ],\n",
